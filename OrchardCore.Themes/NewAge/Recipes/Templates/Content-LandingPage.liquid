{% zone "Header" %}
        <!-- Mashead header-->
        <header class="masthead">
            <div class="container px-5">
                <div class="row gx-5 align-items-center">
                    <div class="col-lg-6">
                        <!-- Mashead text and app badges-->
                        <div class="mb-5 mb-lg-0 text-center text-lg-start">
                            <h1 class="display-1 lh-1 mb-3">{{ Model.ContentItem.Content.LandingPage.Head.Text }}</h1>
                            <p class="lead fw-normal text-muted mb-5">{{ Model.ContentItem.Content.LandingPage.SubHead.Text }}</p>
                            <div class="d-flex flex-column flex-lg-row align-items-center">                                
                                {% if Model.ContentItem.Content.AppDownloadUrls.ContentItems.size > 0 %}                                
                                    {% for downloadUrl in Model.ContentItem.Content.AppDownloadUrls.ContentItems %}
                                        <a class="{{ downloadUrl.AppDownload.CssClass.Text}}" href="{{ downloadUrl.AppDownload.Url.Url}}" title="{{ downloadUrl.DisplayText}}" >
                                            <img class="app-badge" src="{{ downloadUrl.AppDownload.BackgroundImage.Paths.first | asset_url}}" alt="{{ downloadUrl.DisplayText}}" />
                                        </a>
                                    {% endfor %}
                                {% endif %}                            
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <!-- Masthead device mockup feature-->
                        <div class="masthead-device-mockup">
                            <svg class="circle" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                <defs>
                                    <linearGradient id="circleGradient" gradientTransform="rotate(45)">
                                        <stop class="gradient-start-color" offset="0%"></stop>
                                        <stop class="gradient-end-color" offset="100%"></stop>
                                    </linearGradient>
                                </defs>
                                <circle cx="50" cy="50" r="50"></circle></svg
                            ><svg class="shape-1 d-none d-sm-block" viewBox="0 0 240.83 240.83" xmlns="http://www.w3.org/2000/svg">
                                <rect x="-32.54" y="78.39" width="305.92" height="84.05" rx="42.03" transform="translate(120.42 -49.88) rotate(45)"></rect>
                                <rect x="-32.54" y="78.39" width="305.92" height="84.05" rx="42.03" transform="translate(-49.88 120.42) rotate(-45)"></rect></svg
                            ><svg class="shape-2 d-none d-sm-block" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="50"></circle></svg>
                            <div class="device-wrapper">
                                <div class="device" data-device="iPhoneX" data-orientation="portrait" data-color="black">
                                    <div class="screen bg-black">
                                        <!-- PUT CONTENTS HERE:-->
                                        <!-- * * This can be a video, image, or just about anything else.-->
                                        <!-- * * Set the max width of your media to 100% and the height to-->
                                        <!-- * * 100% like the demo example below.-->
                                        <video muted="muted" autoplay="" loop="" style="max-width: 100%; height: 100%"><source src="{{ Model.ContentItem.Content.LandingPage.DemoVideo.Paths.first | asset_url }}" type="video/mp4" /></video>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>
{% endzone %}
<!-- Quote/testimonial aside-->
        <aside class="text-center bg-gradient-primary-to-secondary">
            <div class="container px-5">
                <div class="row gx-5 justify-content-center">
                    <div class="col-xl-8">
                        <div class="h2 fs-1 text-white mb-4">"{{ Model.ContentItem.Content.Testimonial.ContentItems[0].Testimonial.Quote.Text }}"</div>
                        <img src="{{ Model.ContentItem.Content.Testimonial.ContentItems[0].Testimonial.Logo.Paths.first | asset_url }}" alt="{{ Model.ContentItem.Content.Testimonial.ContentItems[0].DisplayText }}" title="{{ Model.ContentItem.Content.Testimonial.ContentItems[0].DisplayText }}" style="height: 3rem" />
                    </div>
                </div>
            </div>
        </aside>
        <!-- App features section-->
        <section id="features">
            <div class="container px-5">
                <div class="row gx-5 align-items-center">
                    <div class="col-lg-8 order-lg-1 mb-5 mb-lg-0">
                        <div class="container-fluid px-5">
                            <div class="row gx-5">
                                {% if Model.ContentItem.Content.Features.ContentItems.size > 0 %}                                
                                {% for feature in Model.ContentItem.Content.Features.ContentItems %}                              
                                <div class="col-md-6 mb-5">
                                    <!-- Feature item-->
                                    <div class="text-center">
                                        <i class="{{ feature.Feature.IconClass.Text }} icon-feature text-gradient d-block mb-3"></i>
                                        <h3 class="font-alt">{{ feature.DisplayText }}</h3>
                                        <p class="text-muted mb-0">{{ feature.Feature.Copy.Text }}</p>
                                    </div>
                                </div>
                                {% endfor %}
                                {% endif %}                           
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 order-lg-0">
                        <!-- Features section device mockup-->
                        <div class="features-device-mockup">
                            <svg class="circle" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
                                <defs>
                                    <linearGradient id="circleGradient" gradientTransform="rotate(45)">
                                        <stop class="gradient-start-color" offset="0%"></stop>
                                        <stop class="gradient-end-color" offset="100%"></stop>
                                    </linearGradient>
                                </defs>
                                <circle cx="50" cy="50" r="50"></circle></svg
                            ><svg class="shape-1 d-none d-sm-block" viewBox="0 0 240.83 240.83" xmlns="http://www.w3.org/2000/svg">
                                <rect x="-32.54" y="78.39" width="305.92" height="84.05" rx="42.03" transform="translate(120.42 -49.88) rotate(45)"></rect>
                                <rect x="-32.54" y="78.39" width="305.92" height="84.05" rx="42.03" transform="translate(-49.88 120.42) rotate(-45)"></rect></svg
                            ><svg class="shape-2 d-none d-sm-block" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg"><circle cx="50" cy="50" r="50"></circle></svg>
                            <div class="device-wrapper">
                                <div class="device" data-device="iPhoneX" data-orientation="portrait" data-color="black">
                                    <div class="screen bg-black">
                                        <!-- PUT CONTENTS HERE:-->
                                        <!-- * * This can be a video, image, or just about anything else.-->
                                        <!-- * * Set the max width of your media to 100% and the height to-->
                                        <!-- * * 100% like the demo example below.-->
                                        <video muted="muted" autoplay="" loop="" style="max-width: 100%; height: 100%"><source src="{{ Model.ContentItem.Content.LandingPage.DemoVideo.Paths.first | asset_url }}" type="video/mp4" /></video>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- Basic features section-->
        {% if Model.ContentItem.Content.Promos.ContentItems.size > 0 %}
        {% for promo in Model.ContentItem.Content.Promos.ContentItems %}
        <section class="bg-light">
            <div class="container px-5">
                <div class="row gx-5 align-items-center justify-content-center justify-content-lg-between">
                    <div class="col-12 col-lg-5">
                        <h2 class="display-4 lh-1 mb-4">{{ promo.DisplayText }}</h2>
                        <p class="lead fw-normal text-muted mb-5 mb-lg-0"> {{ promo.Promo.Copy.Html | raw }}</p>
                    </div>                   
                    <div class="col-sm-8 col-md-6">
                        <div class="px-5 px-sm-0"><img class="img-fluid rounded-circle" src="{{ promo.Promo.Image.Paths.first | asset_url }}" alt="{{ promo.DisplayText }}" /></div>
                    </div>
                </div>
            </div>
        </section>
        {% endfor %}
        {% endif %}
        {% if Model.ContentItem.Content.CTA.ContentItems.size > 0 %}
        {% for cta in Model.ContentItem.Content.CTA.ContentItems %}     
        <!-- Call to action section-->        
        <section class="cta" style="background-image:{{ cta.CTA.BackgroundImage.Paths.first | asset_url }} !important">
            <div class="cta-content">
                <div class="container px-5">
                    <h2 class="text-white display-1 lh-1 mb-4">
                       {{ cta.DisplayText | raw }}
                    </h2>                 
                    <a class="btn btn-outline-light py-3 px-4 rounded-pill" href="{{cta.CTA.Url.Url}}" target="_blank">{{cta.CTA.Url.Text}}</a>
                </div>
            </div>
        </section>
        {% endfor %}
        {% endif %}
        <!-- App badge section-->
        <section class="bg-gradient-primary-to-secondary" id="download">
            <div class="container px-5">
                <h2 class="text-center text-white font-alt mb-4">{{ Model.ContentItem.Content.LandingPage.BageTitle.Text}}</h2>
                <div class="d-flex flex-column flex-lg-row align-items-center justify-content-center">
                
                    {% if Model.ContentItem.Content.AppDownloadUrls.ContentItems.size > 0 %}                                
                        {% for downloadUrl in Model.ContentItem.Content.AppDownloadUrls.ContentItems %}
                            <a class="{{ downloadUrl.AppDownload.CssClass.Text}}" href="{{ downloadUrl.AppDownload.Url.Url}}" title="{{ downloadUrl.DisplayText}}" >
                                <img class="app-badge" src="{{ downloadUrl.AppDownload.BackgroundImage.Paths.first | asset_url}}" alt="{{ downloadUrl.DisplayText}}" />
                            </a>
                        {% endfor %}
                    {% endif %} 
                </div>
            </div>
        </section>